{
    "name": "yolkbot",
    "description": "create a shell shockers (self) bot in under 10 lines.",
    "version": "0.0.2",
    "keywords": ["shell shockers", "shellshock.io", "blue wizard", "yolkbot"],
    "homepage": "https://github.com/enbyte/yolkbot#readme",
    "bugs": {
        "url": "https://github.com/enbyte/yolkbot/issues"
    },
    "author": "hijinks",
    "contributors": ["1ust"],
    "repository": "github:enbyte/yolkbot",
    "type": "module",
    "files": [
        "data/*.*",
        "src/**/*",
        "package.json",
        "README.md"
    ],
    "exports": {
        "./package.json": "./package.json",
        ".": "./src/index.js",
        "./api": "./src/api.js",
        "./bot": "./src/bot.js",
        "./comm": "./src/comm/index.js",
        "./manager": "./src/manager.js",
        "./matchmaker": "./src/matchmaker.js",
        "./packets": "./src/packet.js",
        "./constants": "./src/constants/index.js",
        "./constants/*": "./src/constants/*.js",
        "./constants/*.js": "./src/constants/*.js",
        "./dispatch": "./src/dispatches/index.js",
        "./dispatch/*": "./src/dispatches/*.js",
        "./dispatch/*.js": "./src/dispatches/*.js"
    },
    "dependencies": {
        "socks-proxy-agent": "^8.0.5",
        "ws": "^8.18.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.20.0",
        "@stylistic/eslint-plugin": "^4.0.1",
        "eslint": "^9.20.1",
        "globals": "^15.15.0"
    },
    "scripts": {
        "lint": "eslint --fix",
        "prod": "./prod.sh",
        "test": "./build.sh"
    },
    "imports": {
        "#api": "./src/api.js",
        "#bot": "./src/bot.js",
        "#constants": "./src/constants/index.js",
        "#dispatch/*": "./src/dispatches/*",
        "#manager": "./src/manager.js",
        "#matchmaker": "./src/matchmaker.js",
        "#packet": "./src/packet.js"
    }
}